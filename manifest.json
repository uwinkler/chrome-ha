{
  "manifest_version": 3,
  "name": "Home Assistant",
  "version": "1.0.0",
  "permissions": ["activeTab"],
  "action": {
    "default_title": "Click to open the HA open panel",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": ["<all_urls>"]
}
